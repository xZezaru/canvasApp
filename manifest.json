{
    "name": "canvasApp",
    "version": "1.0.1",
    "description": "canvas app to paint in Firefox",
    "manifest_version": 2,
    "permissions": [
        "<all_urls>",
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "browser_style": true,
        "default_title": "canvasApp",
        "default_popup": "canvas.html"
    },
    "icons": {
        "48": "icons/icon-phone-keypad.png"
    }
}